lines = Read().SplitLines()
i = 1
while i < lines.Size()
	NK = lines.Get(i).SplitWords().Map(\word. return word.Int())
	N = NK.Get(0)
	K = NK.Get(1)
	a = lines.Get(i+1).SplitWords().Map(\word. return word.Int())

	total = 0
	j = 0
	while j < a.Size()
		if a.Get(j) <= 0
			total = total + 1
		j = j + 1

	if total < K
		"YES".Print()
	else
		"NO".Print()
	i = i + 2
