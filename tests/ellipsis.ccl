[a, b, c...] = [1, 2, 3, 4, 5]

Assert(a == 1, "a == 1")
Assert(b == 2, "b == 2")
Assert(c == [3, 4, 5], "c == [3, 4, 5]")


looped = False
[[1, 2, 3, 4, 5]].Each() \\ [a, b, c...]
	looped = True
	Assert(a == 1, "a == 1")
	Assert(b == 2, "b == 2")
	Assert(c == [3, 4, 5], "c == [3, 4, 5]")
Assert(looped, "looped")
